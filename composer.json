{
    "name": "isaeken/color",
    "version": "1.0",
    "description": "Color tools library",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/isaeken/color",
    "readme": "README.md",
    "authors": [
        {
            "name": "Isa Eken",
            "email": "hello@isaeken.com.tr",
            "homepage": "https://www.isaeken.com.tr",
            "role": "Founder"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.53"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "IsaEken\\Color\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsaEken\\Color\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
